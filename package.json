{
  "name": "vue-stock-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx server/index.ts",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "type-check": "vue-tsc -b --noEmit",
    "format": "prettier --write '**/*.{ts,vue}'"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "1.9.0",
    "crypto-js": "4.2.0",
    "element-plus": "2.9.8",
    "jsencrypt": "3.0.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "3.5.13",
    "vue-router": "^4.5.0",
    "express": "^4.18.2",
    "iconv-lite": "^0.6.3",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.0.5",
    "vitest": "^3.1.2",
    "vue-eslint-parser": "^10.0.0",
    "vue-tsc": "^2.2.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0",
    "tsx": "^4.20.3"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
